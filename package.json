{
  "name": "vact-core",
  "version": "1.0.0",
  "description": "一个简单的前端响应式新框架",
  "main": "dist/boundle.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "pnpm examples",
    "test": "jest --passWithNoTests",
    "build": "pnpm build:vactapp",
    "build:vactapp": "cross-env NODE_ENV=production USEING_CONFIG=vactapp rollup -c",
    "examples": "cross-env NODE_ENV=production USEING_CONFIG=examples rollup -c"
  },
  "keywords": [
    "前端框架",
    "响应式",
    "响应式前端框架"
  ],
  "author": "烟云雾沧海",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.0.0",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "repository": "https://github.com/yanyunwu/vact",
  "files": [
    "dist",
    "src",
    "index.d.ts",
    "index.ts",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "vactapp": "workspace:*"
  }
}
